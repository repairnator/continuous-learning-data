+
+    @Override
+    S negate();
+
+    @Override
+    S power(int power);
+
+    @Override
+    S signum();
+
+    @Override
+    S subtract(double scalarSubtrahend);
+
+    @Override
+    S subtract(float scalarSubtrahend);
+
+    @Override
+    S subtract(S scalarSubtrahend);
+
+}
